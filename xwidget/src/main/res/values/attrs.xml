        <attr name="XViewTemplate_gradient_radius" format="dimension"/>
        <attr name="XViewTemplate_stroke_color" format="color" />
        <attr name="XViewTemplate_stroke_border" format="dimension"/>
        <attr name="XViewTemplate_stroke_gradient_start_color" format="color"/>
