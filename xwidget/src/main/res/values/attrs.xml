<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="XTextViewCustom" >
        <attr name="XTextViewCustom_corner" format="dimension" />
        <attr name="XTextViewCustom_corner_type">
            <flag name="left_top" value="0x11"/>
            <flag name="right_top" value="0x21"/>
            <flag name="left_bottom" value="0x12"/>
            <flag name="right_bottom" value="0x22"/>
            <flag name="all" value="0"/>
        </attr>

        <attr name="XTextViewCustom_blur_type" format="enum">
            <enum name="normal" value="0"/>
            <enum name="outer" value="1"/>
            <enum name="solid" value="2"/>
            <enum name="inner" value="3"/>
        </attr>

        <attr name="XTextViewCustom_drawable" format="reference"/>
        <attr name="XTextViewCustom_stated_drawable" format="reference"/>

        <attr name="XTextViewCustom_blur_radius" format="dimension"/>
        <attr name="XTextViewCustom_blur_color" format="color"/>

        <attr name="XTextViewCustom_shadow_radius" format="dimension"/>
        <attr name="XTextViewCustom_shadow_dx" format="dimension"/>
        <attr name="XTextViewCustom_shadow_dy" format="dimension"/>
        <attr name="XTextViewCustom_shadow_color" format="color"/>

        <attr name="XTextViewCustom_solid_color" format="color" />
        <attr name="XTextViewCustom_solid_gradient" format="enum">
            <enum name="linear" value="0"/>
            <enum name="sweep" value="1"/>
        </attr>
        <attr name="XTextViewCustom_gradient_start_color" format="color"/>
        <attr name="XTextViewCustom_gradient_center_color" format="color"/>
        <attr name="XTextViewCustom_gradient_end_color" format="color"/>
        <attr name="XTextViewCustom_gradient_orientation" format="enum">
            <enum name="top_bottom" value="2"/>
            <enum name="left_right" value="1"/>
        </attr>
        <attr name="XTextViewCustom_gradient_radius" format="dimension"/>
        <attr name="XTextViewCustom_stroke_color" format="color" />
        <attr name="XTextViewCustom_stroke_border" format="dimension"/>
        <attr name="XTextViewCustom_text_color" format="color" />
        <attr name="XTextViewCustom_color_filter" format="color" />


        <attr name="XTextViewCustom_stroke_gradient_start_color" format="color"/>
        <attr name="XTextViewCustom_stroke_gradient_middle_color" format="color"/>
        <attr name="XTextViewCustom_stroke_gradient_end_color" format="color"/>
        <attr name="XTextViewCustom_stroke_gradient_offset_x" format="dimension"/>
        <attr name="XTextViewCustom_stroke_gradient_offset_y" format="dimension"/>
        <attr name="XTextViewCustom_stroke_gradient_offset_x1" format="dimension"/>
        <attr name="XTextViewCustom_stroke_gradient_offset_y1" format="dimension"/>

        <attr name="XTextViewCustom_state" format="enum">
            <enum name="none" value="0"/>
            <enum name="pressed" value="1"/>
            <enum name="selected" value="2"/>
            <enum name="checked" value="3"/>
        </attr>
        <attr name="XTextViewCustom_stated_corner" format="dimension" />
        <attr name="XTextViewCustom_stated_corner_type">
            <flag name="left_top" value="0x11"/>
            <flag name="right_top" value="0x21"/>
            <flag name="left_bottom" value="0x12"/>
            <flag name="right_bottom" value="0x22"/>
            <flag name="all" value="0"/>
        </attr>

        <attr name="XTextViewCustom_stated_shadow_radius" format="dimension"/>
        <attr name="XTextViewCustom_stated_shadow_dx" format="dimension"/>
        <attr name="XTextViewCustom_stated_shadow_dy" format="dimension"/>
        <attr name="XTextViewCustom_stated_shadow_color" format="color"/>

        <attr name="XTextViewCustom_stated_blur_type" format="enum">
            <enum name="normal" value="0"/>
            <enum name="outer" value="1"/>
            <enum name="solid" value="2"/>
            <enum name="inner" value="3"/>
        </attr>

        <attr name="XTextViewCustom_stated_blur_radius" format="dimension"/>
        <attr name="XTextViewCustom_stated_blur_color" format="color"/>

        <attr name="XTextViewCustom_stated_solid_color" format="color"/>
        <attr name="XTextViewCustom_stated_solid_gradient" format="enum">
            <enum name="linear" value="0"/>
            <enum name="sweep" value="1"/>
        </attr>
        <attr name="XTextViewCustom_stated_gradient_start_color" format="color"/>
        <attr name="XTextViewCustom_stated_gradient_center_color" format="color"/>
        <attr name="XTextViewCustom_stated_gradient_end_color" format="color"/>
        <attr name="XTextViewCustom_stated_gradient_orientation" format="enum">
            <enum name="top_bottom" value="2"/>
            <enum name="left_right" value="1"/>
        </attr>
        <attr name="XTextViewCustom_stated_gradient_radius" format="dimension"/>
        <attr name="XTextViewCustom_stated_stroke_color" format="color" />
        <attr name="XTextViewCustom_stated_stroke_border" format="dimension"/>

        <attr name="XTextViewCustom_stated_stroke_gradient_start_color" format="color"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_middle_color" format="color"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_end_color" format="color"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_offset_x" format="dimension"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_offset_y" format="dimension"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_offset_x1" format="dimension"/>
        <attr name="XTextViewCustom_stated_stroke_gradient_offset_y1" format="dimension"/>

        <attr name="XTextViewCustom_stated_text_color" format="color" />
        <attr name="XTextViewCustom_stated_color_filter" format="color" />
    </declare-styleable>
</resources>